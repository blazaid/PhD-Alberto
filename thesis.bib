	issn = {0144-1647},
	journal = {Transport Reviews},
	keywords = {car-following models,driving behaviour,heavy vehicles,heterogeneous traffic,traffic flow modelling},
	number = {May 2015},
	pages = {37--41},
	title = {{Transport Reviews : A Transnational A State-of-the-Art Review of Car- Following Models with Particular Considerations of Heavy Vehicles}},
