@article{Das2009,
	abstract = {We report on simulations of chaotic traffic flow in freeways based$\backslash$non a fuzzy knowledge based model of driver behavior. Our simulation$\backslash$napproach is a microscopic method similar to Q. Yang and H.N.$\backslash$nKoutsopoulos (1996), where individual vehicles are separately modeled.$\backslash$nThe knowledge base consists of an extensive and exhaustive set of fuzzy$\backslash$nIF-THEN rules depicting all possible traffic situations that a driver is$\backslash$nlikely to encounter in a highway. The principal motivation behind using$\backslash$na fuzzy knowledge based approach to model the driver behavior is because$\backslash$nfuzzy modeling provides an effective means to break up any highly$\backslash$nnonlinear system into IF-THEN rules (J.M. Mendel, 1995). In addition,$\backslash$nfuzzy logic is well equipped to handle uncertainties that are present in$\backslash$nreal world traffic situations (P. Chakraborty and S. Kikuchi, 1992; S.$\backslash$nKikuchi and M. Pursula, 1998). Therefore using fuzzy rules, it is$\backslash$npossible to incorporate linguistic descriptions of scenarios such as$\backslash$n`speed is moderate' or `adjacent lane gap is quite acceptable'. Instead$\backslash$nof quantifying variables into crisp classes as in a traditional expert$\backslash$nsystem, this is the manner in which a driver is more likely to perceive$\backslash$nany situation. Fuzzy logic techniques are receiving recent attention as$\backslash$nimportant tools in transportation engineering studies and such$\backslash$napproaches have met with a great deal of success},
	author = {Das, S. and Bowles, B.a.},
	doi = {10.1109/NAFIPS.1999.781668},
	isbn = {0-7803-5211-4},
	journal = {18th International Conference of the North American Fuzzy Information Processing Society - NAFIPS (Cat. No.99TH8397)},
	pages = {130--133},
	publisher = {IEEE},
	title = {{Simulations of highway chaos using fuzzy logic}},
	url = {http://ieeexplore.ieee.org/document/781668/},
	year = {1999}
}

@article{Wu2003,
	abstract = {The use of microscopic simulation models to assess the likely effects of new traffic management applications and changes in vehicle technology is becoming increasingly popular. However the validity of the models is a topic of increasing concern, as the quality of the presentation often exceeds the models ability to predict what is likely to happen. Traditionally, model validity has been ascertained through comparing outputs aggregated at a macroscopic level such as speed flow and lane use, against real data. Little microscopic comparison is generally possible and, where this is done there is often no separation of the calibration and validation process. This paper demonstrates how microscopic validation may be undertaken when suitable data is available, in this case time series data collected by an instrumented vehicle, and its use in the validation of the car following performance of a fuzzy logic based car following model. Good agreement has been attained between the simulated model and observed data, primarily using a root mean square error indicator. Lastly, a brief comparison of the new model with the performance of a number of existing formulations has also been undertaken. ?? 2003 Elsevier Ltd. All rights reserved.},
	author = {Wu, J and Brackstone, M and McDonald, M},
	doi = {10.1016/j.trc.2003.05.001},
	isbn = {0968-090X},
	issn = {0968090X},
	journal = {Transportation Research Part C: Emerging Technologies},
	keywords = {Calibration,Car following,Instrumented vehicle,Simulation,Validation},
	number = {6},
	pages = {463--479},
	title = {{The validation of a microscopic simulation model: A methodological case study}},
	url = {http://www.sciencedirect.com/science/article/pii/S0968090X03000585},
	volume = {11},
	year = {2003}
}

